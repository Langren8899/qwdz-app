<!--pages/service/service.wxml-->
<block wx:for="{{list}}">
	<view class="goods_box">
		<image src="{{item.url}}" class="goods-thumb" mode="aspectFill"></image>
	</view>
	<view class="goods-tab-box">
		<view class="goods_details" data-index="0">{{item.title}}</view>
		<view class="goods-content">
			<view>{{item.content}}</view>
		</view>
	</view>
</block>